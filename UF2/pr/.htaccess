# Habilitar reescritura de URLs
RewriteEngine On

# Establecer directorio base
RewriteBase /pr/

# Si el archivo o directorio existe, permitir acceso directo
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirigir todas las demás solicitudes a index.php con parámetro URL
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]