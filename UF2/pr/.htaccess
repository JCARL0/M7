# Enable mod_rewrite
RewriteEngine On

# Set the base directory (change "FINAL" if your project folder is different)
RewriteBase /pr/

# If the request is for an existing file or directory, allow it
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirect all other requests to index.php and pass URL as a parameter
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
